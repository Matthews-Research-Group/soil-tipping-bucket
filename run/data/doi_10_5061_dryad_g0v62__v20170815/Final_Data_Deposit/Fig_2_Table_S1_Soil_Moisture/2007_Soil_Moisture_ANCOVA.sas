Data soystats2007;
Infile 'C:\Users\sbgray\Documents\Combined Soil H2O and driFACE manuscript\Raw data and code\Final\Fig_2_Table_S1_Soil_Moisture\2007_soil_moisture_ringmeans.csv'
dlm=',' firstobs=2 lrecl=10000; input Obs ring doy CO2 block  
true_ten true_twenty true_thirty true_forty true_fifty true_sixty true_seventy 
start_ten start_twenty start_thirty start_forty start_fifty start_sixty start_seventy @@;

proc sort data=soystats2007;
by block doy CO2;
run;
********************************************5-15 cm soil depth ANCOVA********************;
proc mixed data=soystats2007 order=data;
class block doy CO2;
model true_ten = doy co2 doy*co2 start_ten/ ddfm=kr;
random block block*doy;
repeated doy / type=cs subject=block*co2;
lsmeans doy*co2/pdiff;
ods output diffs = co2effect_ten_2007 lsmeans = lsmeans_ten_2007;
ods listing exclude diffs;
run;
proc print data=co2effect_ten_2007; where doy = _doy;
run;
