options ls=170 ps=10000 pageno=1;
title 'driFACE 2011 Water Potential ';
Data driFACE2011wp;
Infile 'C:\Users\sbgray\Documents\Combined Soil H2O and driFACE manuscript\Raw data and code\Final\Table_S12_water_potential\dri2011WPringmeans.csv' 
dlm=',' firstobs=2 lrecl=10000; input DOY block ring CO2$ H2O$ WP OP TP@;
run;
*************Water potential ANOVA;
proc mixed data =driFACE2011wp order =data;
class DOY block co2 H2O ;
model WP = DOY|CO2|H2O  / ddfm =kr;
random block block*co2 ;
lsmeans DOY|CO2|H2O/ pdiff; 
ods output lsmeans = WPseason;
ods output diffs = WPseasondiffs;
run ;
ODS CSV file='C:\Users\sbgray\Documents\Summer 2011\Water Potential\Figures\WP_lsmeans.csv';
proc print data = WPseason; run;ODS CSV close;
ODS CSV file='C:\Users\sbgray\Documents\Summer 2011\Water Potential\Figures\WP_pdiffs.csv';
proc print data = WPseasondiffs; run;ODS CSV close;
